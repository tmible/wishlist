{
	"name": "@tmible/wishlist-portal",
	"version": "1.1.3",
	"private": true,
	"description": "Портал со списками желаний",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tmible/wishlist.git",
		"directory": "portal"
	},
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"start:background": "SERVICE_NAME=\"wishlist-portal\" SERVICE_DESCRIPTION=\"Tmible's wishlist portal\" PATH_TO_EXECUTABLE=\"./build\" ../deploy/start-systemd-service.sh",
		"restart:background": "systemctl restart wishlist-portal",
		"stop:background": "systemctl stop wishlist-portal",
		"lint": "eslint ./src",
		"lint:fix": "eslint --fix ./src",
		"test": "vitest run --coverage",
		"test:watch": "vitest --coverage",
		"test:prod": "vitest run",
		"update": "npm-check-updates"
	},
	"dependencies": {
		"@tiptap/core": "^2.6.6",
		"@tiptap/extension-blockquote": "^2.6.6",
		"@tiptap/extension-bold": "^2.6.6",
		"@tiptap/extension-bubble-menu": "^2.6.6",
		"@tiptap/extension-code": "^2.6.6",
		"@tiptap/extension-code-block": "^2.6.6",
		"@tiptap/extension-document": "^2.6.6",
		"@tiptap/extension-history": "^2.6.6",
		"@tiptap/extension-italic": "^2.6.6",
		"@tiptap/extension-link": "^2.6.6",
		"@tiptap/extension-paragraph": "^2.6.6",
		"@tiptap/extension-placeholder": "^2.6.6",
		"@tiptap/extension-strike": "^2.6.6",
		"@tiptap/extension-text": "^2.6.6",
		"@tiptap/extension-typography": "^2.6.6",
		"@tiptap/extension-underline": "^2.6.6",
		"@tiptap/pm": "^2.6.6",
		"@tmible/wishlist-common": "workspace:*",
		"@use-gesture/vanilla": "^10.3.1",
		"better-sqlite3": "^11.1.2",
		"bits-ui": "^0.21.13",
		"jsonwebtoken": "^9.0.2",
		"lucide-svelte": "^0.429.0",
		"open-color": "^1.9.1"
	},
	"devDependencies": {
		"@stylistic/eslint-plugin-js": "^1.8.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.14",
		"@testing-library/svelte": "^5.2.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/eslint": "^8.56.10",
		"@vitest/coverage-v8": "^2.0.0",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.10",
		"eslint": "^8.57.0",
		"eslint-plugin-array-func": "^4.0.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsdoc": "^48.5.0",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-no-secrets": "^0.8.9",
		"eslint-plugin-optimize-regex": "^1.2.1",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-promise": "^6.2.0",
