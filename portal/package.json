		"eslint-plugin-unicorn": "^54.0.0",
		"globals": "^15.0.0",
		"jsdom": "^24.1.0",
		"npm-check-updates": "^16.14.20",
		"postcss": "^8.4.41",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.0"
	},
	"packageManager": "pnpm@9.4.0",
	"engines": {
		"node": "22.3.0",
		"pnpm": "9.4.0"
	}
