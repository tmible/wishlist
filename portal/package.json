{
	"name": "@tmible/wishlist-portal",
	"version": "1.4.2",
	"private": true,
	"description": "Портал со списками желаний",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tmible/wishlist.git",
		"directory": "portal"
	},
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm \"include pino SQLite worker to build\"",
		"include pino SQLite worker to build": "cp src/lib/server/pino-sqlite-transport.worker.js build/server/chunks",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"start:background": "SERVICE_NAME=\"wishlist-portal\" SERVICE_DESCRIPTION=\"Tmible's wishlist portal\" PATH_TO_EXECUTABLE=\"./build\" ../deploy/start-systemd-service.sh",
		"restart:background": "systemctl restart wishlist-portal",
		"stop:background": "systemctl stop wishlist-portal",
		"lint": "eslint ./src",
		"lint:fix": "eslint --fix ./src",
		"test": "vitest run --coverage",
		"test:watch": "vitest --coverage",
		"test:prod": "vitest run",
		"update": "npm-check-updates",
		"release": "../scripts/release.sh"
	},
	"dependencies": {
		"@lucide/lab": "0.1.2",
		"@tiptap/core": "2.11.5",
		"@tiptap/extension-blockquote": "2.11.5",
		"@tiptap/extension-bold": "2.11.5",
		"@tiptap/extension-bubble-menu": "2.11.5",
		"@tiptap/extension-code": "2.11.5",
		"@tiptap/extension-code-block": "2.11.5",
		"@tiptap/extension-document": "2.11.5",
		"@tiptap/extension-history": "2.11.5",
		"@tiptap/extension-italic": "2.11.5",
		"@tiptap/extension-link": "2.11.5",
		"@tiptap/extension-paragraph": "2.11.5",
		"@tiptap/extension-placeholder": "2.11.5",
		"@tiptap/extension-strike": "2.11.5",
		"@tiptap/extension-text": "2.11.5",
		"@tiptap/extension-typography": "2.11.5",
		"@tiptap/extension-underline": "2.11.5",
		"@tiptap/pm": "2.11.5",
		"@tmible/wishlist-common": "workspace:*",
		"@use-gesture/vanilla": "10.3.1",
		"better-sqlite3": "11.8.1",
		"bits-ui": "0.22.0",
		"jsonwebtoken": "9.0.2",
		"lucide-svelte": "0.429.0",
		"open-color": "1.9.1",
		"pino": "9.6.0",
		"pino-abstract-transport": "2.0.0",
		"pino-pretty": "13.0.0",
		"sortablejs": "1.15.6"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.4.1",
		"@eslint/js": "9.19.0",
		"@stylistic/eslint-plugin-js": "3.0.1",
		"@sveltejs/adapter-auto": "3.2.4",
		"@sveltejs/adapter-node": "5.2.12",
		"@sveltejs/kit": "2.5.24",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@tailwindcss/typography": "0.5.16",
		"@testing-library/svelte": "5.2.6",
		"@testing-library/user-event": "14.6.1",
		"@types/eslint": "9.6.1",
		"@vitest/coverage-v8": "3.0.5",
		"autoprefixer": "10.4.20",
		"daisyui": "4.12.23",
		"eslint": "9.19.0",
		"eslint-plugin-array-func": "5.0.2",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-jsdoc": "50.6.3",
		"eslint-plugin-no-only-tests": "3.3.0",
		"eslint-plugin-no-secrets": "2.2.1",
		"eslint-plugin-optimize-regex": "1.2.1",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-regexp": "2.7.0",
		"eslint-plugin-security": "3.0.1",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"eslint-plugin-sonarjs": "3.0.1",
		"eslint-plugin-svelte": "2.46.1",
		"eslint-plugin-unicorn": "56.0.1",
		"globals": "15.14.0",
		"jsdom": "26.0.0",
		"npm-check-updates": "17.1.14",
		"postcss": "8.5.1",
		"svelte": "4.2.18",
		"svelte-check": "3.8.5",
		"tailwindcss": "3.4.17",
		"typescript": "5.7.3",
		"vite": "5.4.1",
		"vitest": "3.0.5"
	},
	"packageManager": "pnpm@10.2.0",
	"engines": {
		"node": "22.13.1",
		"pnpm": "10.2.0"
	}
}
