{
  "name": "@tmible/wishlist-bot/dashboard-server",
  "version": "1.0.0",
  "description": "Сервер с дашбордами для телеграм-бота со списками желаний",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "start:bg": "dotenvx run -- forever -a start --uid \"wishlist-bot-dashboards-server\" build",
    "restart:bg": "dotenvx run -- forever -a restart wishlist-bot-dashboards-server",
    "stop:bg": "forever stop wishlist-bot-dashboards-server",
    "lint": "eslint --ignore-pattern '**/__tests__/**' ./src",
    "lint:fix": "eslint --ignore-pattern '**/__tests__/**' --fix ./src",
    "lint:tests": "eslint --config .eslintrc.tests.json --ext .test.js ./src",
    "lint:tests:fix": "eslint --config .eslintrc.tests.json --ext .test.js --fix ./src",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "test:prod": "vitest run",
    "update": "npm-check-updates"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.8.1",
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/svelte": "^5.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.10",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-array-func": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.5.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-svelte": "^2.41.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "jsdom": "^24.1.0",
    "npm-check-updates": "^16.14.20",
    "postcss": "^8.4.38",
    "postcss-load-config": "^6.0.1",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "svelte-headless-table": "^0.18.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.2",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "engines": {
    "node": "22.3.0"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.45.0",
    "@internationalized/date": "^3.5.4",
    "better-sqlite3": "^11.1.1",
    "bits-ui": "^0.21.10",
    "chart.js": "^4.4.3",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "chartjs-plugin-annotation": "^3.0.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "jsonwebtoken": "^9.0.2",
    "svelte-radix": "^1.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1"
  }
}
