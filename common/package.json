{
	"name": "@tmible/wishlist-common",
	"version": "1.2.1",
	"private": true,
	"description": "Общие элементы для телеграм бота и портала со списками желаний и сервера с дашбордами для них",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tmible/wishlist.git",
		"directory": "common"
	},
	"type": "module",
	"exports": {
		"./*": {
			"types": "./dist/*.d.ts",
			"svelte": "./dist/*.js",
			"import": "./dist/*.js"
		},
		"./constants/*": {
			"types": "./dist/constants/*.const.d.ts",
			"svelte": "./dist/constants/*.const.js",
			"import": "./dist/constants/*.const.js"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"scripts": {
		"package": "svelte-kit sync && svelte-package",
		"prepublishOnly": "pnpm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "eslint ./src",
		"lint:fix": "eslint --fix ./src",
		"test": "vitest run --coverage",
		"test:watch": "vitest --coverage",
		"test:prod": "vitest run",
		"update": "npm-check-updates",
		"release": "../scripts/release.sh"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.4.1",
		"@eslint/js": "9.19.0",
		"@stylistic/eslint-plugin-js": "3.0.1",
		"@sveltejs/adapter-auto": "3.2.4",
		"@sveltejs/kit": "2.5.24",
		"@sveltejs/package": "2.3.4",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@types/eslint": "9.6.1",
		"@vitest/coverage-v8": "3.0.5",
		"eslint": "9.19.0",
		"eslint-plugin-array-func": "5.0.2",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-jsdoc": "50.6.3",
		"eslint-plugin-no-only-tests": "3.3.0",
		"eslint-plugin-no-secrets": "2.2.1",
		"eslint-plugin-optimize-regex": "1.2.1",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-regexp": "2.7.0",
		"eslint-plugin-security": "3.0.1",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"eslint-plugin-sonarjs": "3.0.1",
		"eslint-plugin-svelte": "2.46.1",
		"eslint-plugin-unicorn": "56.0.1",
		"globals": "15.14.0",
		"npm-check-updates": "17.1.14",
		"svelte": "4.2.18",
		"svelte-check": "3.8.5",
		"typescript": "5.7.3",
		"vite": "6.1.0",
		"vitest": "3.0.5"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"packageManager": "pnpm@10.2.0",
	"engines": {
		"node": "22.13.1",
		"pnpm": "10.2.0"
	}
}
